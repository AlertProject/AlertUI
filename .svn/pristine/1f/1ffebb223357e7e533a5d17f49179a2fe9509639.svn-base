.container_wide
{
	margin-left: auto;
	margin-right: auto;
	width: 960px;
	background: #fff;
}
.item_left
{
	margin-left: auto;
	margin-right: auto;
	width: 306px;
	height:500px;
	float:left;
	-moz-box-shadow:0px 0px 3px #aaa;
    -webkit-box-shadow:0px 0px 3px #aaa;
    box-shadow:0px 0px 3px #aaa;
    -moz-border-radius:10px;
    -webkit-border-radius:10px;
    border-radius:10px;
    border:2px solid #fff;
    background-color:#f9f9f9;
    margin: 9px;
    overflow:scroll;
}
.item_right
{
	margin-left: auto;
	margin-right: auto;
	width: 606px;
	height:500px;
	float:right;
	-moz-box-shadow:0px 0px 3px #aaa;
    -webkit-box-shadow:0px 0px 3px #aaa;
    box-shadow:0px 0px 3px #aaa;
    -moz-border-radius:10px;
    -webkit-border-radius:10px;
    border-radius:10px;
    border:2px solid #fff;
    background-color:#f9f9f9;
    margin: 9px;
    border: 0 !important;
}
.timeline
{
	margin-left: auto;
	margin-right: auto;
	width: 940px;
	height:200px;
	float:right;
	-moz-box-shadow:0px 0px 3px #aaa;
    -webkit-box-shadow:0px 0px 3px #aaa;
    box-shadow:0px 0px 3px #aaa;
    -moz-border-radius:10px;
    -webkit-border-radius:10px;
    border-radius:10px;
    border:2px solid #fff;
    background-color:#fff;
    margin: 9px;
    padding-top:10px;
}
#graph-controls-div {
	position:absolute;
	height:20px;
	z-index:50;
	margin:5px;
}
.graph
{
	width: 606px;
	height:400px;
	float:left;
	-moz-box-shadow:0px 0px 3px #aaa;
    -webkit-box-shadow:0px 0px 3px #aaa;
    box-shadow:0px 0px 3px #aaa;
    -moz-border-radius:10px;
    -webkit-border-radius:10px;
    border-radius:10px;
    border:2px solid #fff;
    background-color:#f9f9f9;
    margin:9px;
}
.cloud
{
	margin-left: auto;
	margin-right: auto;
	width: 306px;
	height:400px;
	float:right;
	-moz-box-shadow:0px 0px 3px #aaa;
    -webkit-box-shadow:0px 0px 3px #aaa;
    box-shadow:0px 0px 3px #aaa;
    -moz-border-radius:10px;
    -webkit-border-radius:10px;
    border-radius:10px;
    border:2px solid #fff;
    background-color:#f9f9f9;
    margin: 9px;
}
#item_details {
	float:center;
	width:97%;
	height:480px;
}
html body * span.clear,
html body * div.clear,
html body * li.clear,
html body * dd.clear
{
	background: none;
	border: 0;
	clear: both;
	display: block;
	float: none;
	font-size: 0;
	list-style: none;
	margin: 0;
	padding: 0;
	overflow: hidden;
	visibility: hidden;
	width: 0;
	height: 0;
}
.clearfix:after
{
	clear: both;
	content: '.';
	display: block;
	visibility: hidden;
	height: 0;
}
.clearfix
{
	display: inline-block;
}
* html .clearfix
{
	height: 1%;
}
.clearfix
{
	display: block;
}
