/* AutoSuggest CSS - Version 1.2 */
ul.as-selections {
	list-style-type: none;
/*	border-top: 1px solid #888;
	border-bottom: 1px solid #b6b6b6;
	border-left: 1px solid #aaa;
	border-right: 1px solid #aaa; */
	padding: 0px 0 0px 0px !important;
	margin: 0 !important;
	background-color: #F4F4F4;
}
ul.as-selections.loading {
	background-color: #eee;
}
ul.as-selections li {
	float: left;
	margin: 1px 4px 1px 0 !important;
}
ul.as-selections li.as-selection-item {
	color: #93b876;
	font-size: 13px;
	font-family: "Lucida Grande", arial, sans-serif;
	text-shadow: 0 1px 1px #fff;
	border: 1px solid #ccc;
	border-top-color: #ccc;
	padding: 2px 7px 2px 10px;
	border-radius: 12px;
	-webkit-border-radius: 12px;
	-moz-border-radius: 12px;
	box-shadow: 0 1px 1px #e4edf2;
	-webkit-box-shadow: 0 1px 1px #ddd;
	-moz-box-shadow: 0 1px 1px #ddd;
}
ul.as-selections li.as-selection-item.keyword {
	background-color: #A7FFA2;
	background-image: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#A7FFA2), to(#A6FFA2));
}
ul.as-selections li.as-selection-item.concept {
	background-color: #F9FF7E;
	background-image: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#F9FF7E), to(#F9FF7E));
}
ul.as-selections li.as-selection-item.person {
	background-color: #ddeefe;
	background-image: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#ddeefe), to(#bfe0f1));
}
ul.as-selections li.as-selection-item.issue {
	background-color: #FF6140;
	background-image: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#FF6140), to(#AB2400));
}
ul.as-selections li.as-selection-item.source {
	background-color: #FFA8AB;
	background-image: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#FFA8AB), to(#FFA8AB));
}
ul.as-selections li.as-selection-item.product {
	background-color: #B9BAFF;
	background-image: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#B9BAFF), to(#B9BAFF));
}




ul.as-selections li.as-selection-item:last-child {
	margin-left: 30px;
}

ul.as-selections li.as-selection-item a.as-close {
	float: right;
	margin: 1px 0 0 7px;
	padding: 0 2px;
	cursor: pointer;
	color: #fff;
	font-family: "Helvetica", helvetica, arial, sans-serif;
	font-size: 14px;
	font-weight: bold;
	text-shadow: 0 1px 1px #fff;
	-webkit-transition: color .1s ease-in;
}

ul.as-selections li.as-selection-item.blur {
	color: #666666;
	/*background-color: #f4f4f4;
	background-image: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#f4f4f4), to(#d5d5d5));*/
	border-color: #bbb;
	border-top-color: #ccc;
	box-shadow: 0 1px 1px #e9e9e9;
	-webkit-box-shadow: 0 1px 1px #e9e9e9;
	-moz-box-shadow: 0 1px 1px #e9e9e9;
}

ul.as-selections li.as-selection-item.blur a.as-close {
	color: #999;
}

ul.as-selections li:hover.as-selection-item {
	color: #2b3840;
	/*background-color: #bbd4f1;
	background-image: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#bbd4f1), to(#a3c2e5));*/
	border-color: #ddd;
	border-top-color: #eee;
}

ul.as-selections li:hover.as-selection-item a.as-close {
	color: #888;
}

ul.as-selections li.as-selection-item.selected {
	border-color: #ccc
}

ul.as-selections li.as-selection-item a:hover.as-close {
	color: #666;
}

ul.as-selections li.as-selection-item a:active.as-close {
	color: #ccc;
}

ul.as-selections li.as-original {
	margin-top: 2px !important;
	margin-left: 0;
	border: none !important;
}

ul.as-selections li.as-original input {
	border: none;
	outline: none;
	font-size: 13px;
	width: 120px;
	height: 18px !important;
	padding-top: 0px !important;
}

ul.as-list {
	position: absolute;
	list-style-type: none;
	margin: 2px 0 0 0;
	padding: 1px;
	font-size: 14px;
	color: #000;
	font-family: "Lucida Grande", arial, sans-serif;
	background-color: #fff;
	background-color: rgba(255,255,255,0.95);
	z-index: 2;
	box-shadow: 0 2px 12px #222;
	-webkit-box-shadow: 0 2px 12px #222;
	-moz-box-shadow: 0 2px 12px #222;
	border-radius: 5px;
	-webkit-border-radius: 5px;
	-moz-border-radius: 5px;
}

li.as-result-item, li.as-message {
	margin: 0 0 0 0;
	padding: 5px 12px;
	background-color: transparent;
	border: 1px solid #fff;
	border-bottom: 1px solid #ddd;
	cursor: pointer;
	border-radius: 5px;
	-webkit-border-radius: 5px;
	-moz-border-radius: 5px;
}

li:first-child.as-result-item {
	margin: 0;
}

li.as-message {
	margin: 0;
	cursor: default;
}

li.as-result-item.active {
	background-color: #93b876;
	background-image: -webkit-gradient(linear, 0% 0%, 0% 64%, from(rgb(110, 129, 245)), to(rgb(62, 82, 242)));
	border-color: #93b876;
	color: #fff;
	text-shadow: 0 1px 2px #666;
}

li.as-result-item em { 
	font-style: normal; 
	background: #444;  
	padding: 0 2px;
	color: #fff;
}

li.as-result-item.active em { 
	background: #93b876;  
	color: #fff;
}

/* Webkit Hacks  */
@media screen and (-webkit-min-device-pixel-ratio:0) {	
	ul.as-selections {
		border-top-width: 2px;
	}
	ul.as-selections li.as-selection-item {
		padding-top: 3px;
		padding-bottom: 3px;
	}
	ul.as-selections li.as-selection-item a.as-close {
		margin-top: -1px;
	}
	ul.as-selections li.as-original input {
		height: 19px;
	}
}

/* Opera Hacks  */
@media all and (-webkit-min-device-pixel-ratio:10000), not all and (-webkit-min-device-pixel-ratio:0) {
	ul.as-list {
		border: 1px solid #888;
	}
	ul.as-selections li.as-selection-item a.as-close {
		margin-left: 4px;
		margin-top: 0;
	}
}

/* IE Hacks  */
ul.as-list {
	border: 1px solid #888\9;
}
ul.as-selections li.as-selection-item a.as-close {
	margin-left: 4px\9;
	margin-top: 0\9;
}

/* Firefox 3.0 Hacks */
ul.as-list,  x:-moz-any-link, x:default { 
	border: 1px solid #888;
}
BODY:first-of-type ul.as-list, x:-moz-any-link, x:default { /* Target FF 3.5+ */
	border: none;
}